## required

* flake8
* the_silver_searcher


ln -s vimrc/_.vimrc ~/.vimrc
ln -s vimrc ~/.vim

git submodule init 
git submodule update

## vim8

./configure --with-features=huge --enable-multibyte --enable-rubyinterp=yes --enable-pythoninterp=yes --enable-python3interp=yes --enable-perlinterp=yes --enable-luainterp=yes --enable-cscope

